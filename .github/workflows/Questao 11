	public static void main(String[] args) {
		int i,j,c;
		i=0;
		j=0;
	  for(i=0;i<4;i++) {
		 c=0;
		 j=0;
		  while(j<3){
			  
			  if(j==1 && c<1) {
				  System.out.println(i+"-"+j);
				  c++;
				  j--;
				  
			  }else {
				  System.out.println(i+"-"+j);
				  j++;
			  }
			 
		  }
		  
	  }
		
		 
	}
